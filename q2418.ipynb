   "source": [
    "from typing import *\n",
    "\n",
    "class Solution:\n",
    "    def sortPeople(self, names: List[str], heights: List[int]) -> List[str]:\n",
    "        pair = sorted([(name,height) for name,height in zip(names,heights)],key=lambda x: x[1], reverse = True)\n",
    "        return [ele[0] for ele in pair]"
   ]
  }
