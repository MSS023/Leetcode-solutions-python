   "source": [
    "from typing import *\n",
    "\n",
    "class Solution:\n",
    "    def findClosestElements(self, arr: List[int], k: int, x: int) -> List[int]:\n",
    "        arr.sort(key = lambda num: abs(num-x))\n",
    "        return sorted(arr[:k])"
   ]
