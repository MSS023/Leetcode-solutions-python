    "class Solution:\n",
    "    def isPalindrome(self, x: int) -> bool:\n",
    "        s=str(x)\n",
    "        for i in range(len(s)//2):\n",
    "            if s[i]!=s[-i-1]:\n",
    "                return False\n",
    "        return True"
   ]
  }
