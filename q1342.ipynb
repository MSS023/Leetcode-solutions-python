   "source": [
    "class Solution:\n",
    "    def numberOfSteps(self, num: int) -> int:\n",
    "        if num==0:\n",
    "            return 0\n",
    "        if num%2==0:\n",
    "            return 1+self.numberOfSteps(num//2)\n",
    "        return 1+self.numberOfSteps(num-1)"
   ]
  }
