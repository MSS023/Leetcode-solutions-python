   "source": [
    "from typing import *\n",
    "\n",
    "class Solution:\n",
    "    def addSpaces(self, s: str, spaces: List[int]) -> str:\n",
    "        ans = \"\"\n",
    "        prev = 0\n",
    "        for idx in spaces:\n",
    "            ans += s[prev:idx] + \" \"\n",
    "            prev = idx\n",
    "        ans += s[spaces[-1]:]\n",
    "        return ans"
   ]
  }
