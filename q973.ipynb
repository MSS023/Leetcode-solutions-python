   "source": [
    "class Solution:\n",
    "    def kClosest(self, points: list[list[int]], k: int) -> list[list[int]]:\n",
    "        def distance(x1,y1,x2,y2):\n",
    "            return (x1-x2)**2 + (y1-y2)**2\n",
    "        points.sort(key=lambda point: distance(point[0],point[1],0,0))\n",
    "        return points[:k]"
   ]
  }
