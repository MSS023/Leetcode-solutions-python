    "class Solution:\n",
    "    def removeDuplicates(self, s: str) -> str:\n",
    "        stack = [s[0]]\n",
    "        for idx in range(1,len(s)):\n",
    "            if stack and stack[-1] == s[idx]:\n",
    "                stack.pop()\n",
    "            else:\n",
    "                stack.append(s[idx])\n",
    "        return \"\".join(stack)"
   ]
  }
