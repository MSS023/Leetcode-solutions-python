   "source": [
    "class Solution:\n",
    "    def reverseWords(self, s: str) -> str:\n",
    "        words = s.split(\" \")\n",
    "        for idx, word in enumerate(words):\n",
    "            words[idx] = word[::-1]\n",
    "        return \" \".join(words)"
   ]
