    "class Solution:\n",
    "    def totalMoney(self, n: int) -> int:\n",
    "        a = 1\n",
    "        res = 0\n",
    "        while n > 0:\n",
    "            total = min(n,7)\n",
    "            res += total*(2*a+total-1) // 2\n",
    "            n -= 7\n",
    "            a += 1\n",
    "        return res"
   ]
