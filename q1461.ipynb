   "source": [
    "class Solution:\n",
    "    def hasAllCodes(self, s: str, k: int) -> bool:\n",
    "        nums=set()\n",
    "        for i in range(len(s)-k+1):\n",
    "            nums.add(s[i:i+k])\n",
    "            if len(nums)==2**k:\n",
    "                break\n",
    "        if len(nums)==2**k:\n",
    "            return True\n",
    "        return False"
   ]
  }
