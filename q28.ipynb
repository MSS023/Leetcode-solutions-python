    "import re\n",
    "\n",
    "class Solution:\n",
    "    def strStr(self, haystack: str, needle: str) -> int:\n",
    "        pattern=re.compile(needle)\n",
    "        match=re.search(pattern,haystack)\n",
    "        if match:\n",
    "            return match.start()\n",
    "        else:\n",
    "            return -1\n",
    "            "
