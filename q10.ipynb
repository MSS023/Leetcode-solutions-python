    "\n",
    "class Solution:\n",
    "    def isMatch(self, s: str, p: str) -> bool:\n",
    "        pattern=re.compile(p)\n",
    "        if pattern.fullmatch(s) is None:\n",
    "            return False\n",
    "        else:\n",
    "            return True"
