    "from math import *\n",
    "\n",
    "class Solution:\n",
    "    def poorPigs(self, buckets: int, minutesToDie: int, minutesToTest: int) -> int:\n",
    "        tmp = log2(buckets)\n",
    "        t = ceil(minutesToTest/minutesToDie)\n",
    "\n",
    "        if t == 1: return ceil(tmp) \n",
    "        return ceil(tmp / log2(t+1)) "
   ]
